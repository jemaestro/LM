<?xml version = "1.0" ?>
<xs:complexType >
	<xs:sequence>
		<xs:element name = "first name" >
			<xs:simpleType>
				<xs:restriction base ="xs: string" >
					<xs:pattern value = " [a-zA-Z ] {10,70} " />
				</xs:restriction>
			</xs:simpleType>
		</xs:element>
		<xs:element name = "last name" type ="xs:string"/>
		<xs:element name ="edad">
			<xs:simpleType>
				<xs:restriction base = "xs:integer"/>
				<xs:minInclusive value = "0" />
				<xs:maxInclusive value = "60"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	</xs:sequence>
</xs:complexType>



